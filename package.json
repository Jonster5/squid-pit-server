{
	"name": "sp-server",
	"version": "1.0.0",
	"main": "dist/index.js",
	"license": "MIT",
	"scripts": {
		"build": "rimraf dist && tsc",
		"preserve": "yarn build",
		"serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
		"prestart": "yarn build",
		"start": "cross-env NODE_ENV=production node dist/index.js"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"rimraf": "^3.0.2"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.2",
		"@types/express": "^4.17.13",
		"@types/node": "^16.11.11",
		"concurrently": "^6.4.0",
		"nodemon": "^2.0.15"
	}
}
